package com.sbz;
import com.sbz.proj.model.*;
global java.util.logging.Logger LOGGER

rule "U have high card"
no-loop
    when
        $ts: TableState(
            $players: players,
            $currentStage: currentStage
        )
        $pos: Possibility($cards: cards, $pokerHand: pokerHand == PokerHand.HIGH_CARD)
    then
        LOGGER.info("imamo nesto");
end